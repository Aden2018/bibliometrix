@article{ ISI:000166653100004,
Author = {Maietta, OW},
Title = {{The decomposition of cost inefficiency into technical and allocative
   components with panel data of Italian dairy farms}},
Journal = {{EUROPEAN REVIEW OF AGRICULTURAL ECONOMICS}},
Year = {{2000}},
Volume = {{27}},
Number = {{4}},
Pages = {{473-495}},
Month = {{DEC}},
Abstract = {{This paper considers the decomposition of cost inefficiency into its
   technical and allocative components. First, the relevant literature is
   surveyed, showing that the shadow price approach has significant
   computational advantages over alternative procedures. To illustrate this
   approach, a shadow cost model is applied to a panel of dairy farms in
   Northern Italy, and cost inefficiency is decomposed into its technical
   and allocative components. The main results from this application are
   that the cost excess owing to inefficiency in the sample is on average
   69 per cent, mainly as a result of technical inefficiency, and that
   allocative inefficiency relates to the under-utilisation of forage crops
   and of purchased feed with respect to hired labour.}},
