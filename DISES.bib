@article{ ISI:A1993LK83400012,
Author = {PAGANO, M},
Title = {{THE FLOTATION OF COMPANIES ON THE STOCK-MARKET - A COORDINATION FAILURE
   MODEL}},
Journal = {{EUROPEAN ECONOMIC REVIEW}},
Year = {{1993}},
Volume = {{37}},
Number = {{5}},
Pages = {{1101-1125}},
Month = {{JUN}},
Abstract = {{In some countries the size of the stock market and the number of listed
   companies have persistently lagged behind the growth of the economy.
   Trading externalities can help explain this fact. An entrepreneur who
   goes public increases risk sharing opportunities for others. This
   externality can lead to an inefficiently low number of listed companies,
   and also generate multiple equilibria when flotation decisions are
   positively correlated across entrepreneurs. This happens if these face
   borrowing constraints and lack liquidity, and thus cannot diversify
   their portfolios unless they go public. It also happens if trading
   shares is costly, so that a sufficient number of issues must be listed
   to lure investors into the market.}},
