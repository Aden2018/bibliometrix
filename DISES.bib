@article{ ISI:000081631500002,
Author = {Jappelli, T and Pagano, M},
Title = {{The welfare effects of liquidity constraints}},
Journal = {{OXFORD ECONOMIC PAPERS-NEW SERIES}},
Year = {{1999}},
Volume = {{51}},
Number = {{3}},
Pages = {{410-430}},
Month = {{JUL}},
Abstract = {{We analyze the welfare implications of liquidity constraints for
   households in an overlapping generations model with growth. In a closed
   economy with exogenous technical progress, liquidity constraints reduce
   welfare if the economy is dynamically inefficient. But if ii is
   dynamically efficient, some degree of financial repression is required
   to maximize steady-state utility, even though some generations are hurt
   in the transition. With endogenous technical progress, financial
   repression may increase welfare even along the transition path, thus
   leading to a Pareto improvement. In this case the optimal degree of
   financial repression increases as the economy grows.}},
