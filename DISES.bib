@article{ ISI:000249018700013,
Author = {Morgan, Jacqueline and Scalzo, Vincenzo},
Title = {{Asymptotical behavior of finite and possible discontinuous economies}},
Journal = {{ECONOMIC THEORY}},
Year = {{2007}},
Volume = {{33}},
Number = {{2}},
Pages = {{407-412}},
Month = {{NOV}},
Abstract = {{We consider a finite economy in which the data are depending on an
   exogenous parameter and the utilities satisfy a condition, previously
   introduced by the authors and called sequential pseudocontinuity, weaker
   than sequential lower semicontinuity and than sequential upper
   semicontinuity. We show that the economy has a nice asymptotical
   behavior, that is: for any sequence (x(n))(n) of exogenous parameters
   converging to x(0), any convergent subsequence of a sequence of
   approximate competitive equilibria of (epsilon( x(n))(n) converges to an
   exact equilibrium of epsilon(x(o)).}},
