@article{ ISI:000291714100005,
Author = {Presbitero, Andrea F. and Zazzaro, Alberto},
Title = {{Competition and relationship lending: Friends or foes?}},
Journal = {{JOURNAL OF FINANCIAL INTERMEDIATION}},
Year = {{2011}},
Volume = {{20}},
Number = {{3}},
Pages = {{387-413}},
Month = {{JUL}},
Abstract = {{Recent empirical findings by Elsas (2005) and Degryse and Ongena (2007)
   document a U-shaped effect of market concentration on relationship
   lending which cannot be easily accommodated by the investment and
   strategic theories of bank lending orientation. In this paper, we
   suggest that this non-monotonicity can be explained by looking at the
   organizational structure of local credit markets. We provide evidence
   that marginal increases in interbank competition are detrimental to
   relationship lending in markets where large and out-of-market banks are
   predominant. By contrast, where relational lending technologies are
   already widely in use in the market by a large group of small mutual
   banks, an increase in competition may drive banks to further cultivate
   their extensive ties with customers. (C) 2010 Elsevier Inc. All rights
   reserved.}},
