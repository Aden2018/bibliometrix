@inproceedings{ ISI:000259327700027,
Author = {Siciliano, Roberta and Aria, Massimo and D'Ambrosio, Antonio},
Editor = {{Brito, P}},
Title = {{Posterior prediction modelling of optimal trees}},
Booktitle = {{COMPSTAT 2008: PROCEEDINGS IN COMPUTATIONAL STATISTICS}},
Year = {{2008}},
