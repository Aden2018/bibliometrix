@article{ ISI:000184745700019,
Author = {Jappelli, T and Pistaferri, L},
Title = {{Tax incentives and the demand for life insurance: evidence from Italy}},
Journal = {{JOURNAL OF PUBLIC ECONOMICS}},
Year = {{2003}},
Volume = {{87}},
Number = {{7-8}},
Pages = {{1779-1799}},
Month = {{AUG}},
Abstract = {{The theoretical literature suggests that taxation can have a large
   impact on household portfolio selection and allocation. In this paper we
   analyze the tax treatment of life insurance, considering the
   cancellation of tax incentives in Italian life insurance contracts for
   investors with high marginal tax rates and the introduction of
   incentives for those with low rates. Using repeated cross-sectional data
   from 1989 to 1998, we find that the tax reforms had no effect on the
   decision to invest in life insurance or the amount invested. The likely
   explanations are the lack of information and lack of commitment to
   long-term investment. (C) 2002 Elsevier B.V. All rights reserved.}},
