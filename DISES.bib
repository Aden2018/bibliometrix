@article{ ISI:000329476800007,
Author = {Asprone, Domenico and Jalayer, Fatemeh and Simonelli, Saverio and
   Acconcia, Antonio and Prota, Andrea and Manfredi, Gaetano},
Title = {{Seismic insurance model for the Italian residential building stock}},
Journal = {{STRUCTURAL SAFETY}},
