@article{ ISI:000345285800003,
Author = {Butler, Jeffrey V. and Guiso, Luigi and Jappelli, Tullio},
Title = {{The role of intuition and reasoning in driving aversion to risk and
   ambiguity}},
Journal = {{THEORY AND DECISION}},
Year = {{2014}},
Volume = {{77}},
Number = {{4, SI}},
Pages = {{455-484}},
Month = {{DEC}},
Abstract = {{Using a large sample of retail investors as well as experimental data we
   find that risk and ambiguity aversion are positively correlated. We
   provide evidence that a common link is decision mode: intuitive thinkers
   tolerate more risk and ambiguity than effortful reasoners. One
   interpretation is that intuitive thinking confers an advantage in risky
   or ambiguous situations. We present supporting lab and field evidence
   that intuitive thinkers outperform others in uncertain environments.
   Finally, we find that risk and ambiguity aversion vary with individual
   characteristics and wealth. The wealthy are less risk averse but more
   ambiguity averse, which has implications for financial puzzles.}},
