@article{ ISI:000224297900005,
Author = {Guiso, L and Jappelli, T and Padula, M and Pagano, M and Martin, P and
   Gourinchas, PO},
Title = {{Financial market integration and economic growth in the EU}},
Journal = {{ECONOMIC POLICY}},
Year = {{2004}},
Number = {{40}},
Pages = {{524-577}},
Month = {{OCT}},
Note = {{Meeting of the Economic Policy Panel, Dublin, IRELAND, APR, 2004}},
Organization = {{Cent Bank Ireland; Trinity Coll Dublin, Inst Int Integrat Studies}},
Abstract = {{The current diversity in the degree of financial development across the
   EU can be a great opportunity at a time where this area is poised to
   become increasingly financially integrated. Integration should
   accelerate the development of the most backward financial markets, and
   allow companies from these countries to access more sophisticated credit
   and security markets. In line with a large recent literature, it is
   reasonable to expect that financial integration will have a `growth
   dividend' in Europe. This paper attempts to quantify this growth
   dividend, using both industry and firm-level data to estimate the
   empirical relationship between financial market development and growth,
   and to gauge how it will distribute itself across countries and sectors.}},
