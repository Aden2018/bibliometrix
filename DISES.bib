@article{ ISI:000364876200004,
Author = {Ellul, Andrew and Jotikasthira, Chotibhak and Lundblad, Christian T. and
   Wang, Yihui},
Title = {{Is Historical Cost Accounting a Panacea? Market Stress, Incentive
   Distortions, and Gains Trading}},
Journal = {{JOURNAL OF FINANCE}},
Year = {{2015}},
Volume = {{70}},
Number = {{6}},
Pages = {{2489-2538}},
Month = {{DEC}},
Abstract = {{Accounting rules, through their interactions with capital regulations,
   affect financial institutions' trading behavior. The insurance industry
   provides a laboratory to explore these interactions: life insurers have
   greater flexibility than property and casualty insurers to hold
   speculative-grade assets at historical cost, and the degree to which
   life insurers recognize market values differs across U.S. states. During
   the financial crisis, insurers facing a lesser degree of market value
   recognition are less likely to sell downgraded asset-backed securities.
   To improve their capital positions, these insurers disproportionately
   resort to gains trading, selectively selling otherwise unrelated bonds
   with high unrealized gains, transmitting shocks across markets.}},
