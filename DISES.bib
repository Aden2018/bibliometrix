@article{ ISI:000338925900010,
Author = {Acconcia, Antonio and Corsetti, Giancarlo and Simonelli, Saverio},
Title = {{Mafia and Public Spending: Evidence on the Fiscal Multiplier from a
   Quasi-Experiment}},
Journal = {{AMERICAN ECONOMIC REVIEW}},
Year = {{2014}},
Volume = {{104}},
Number = {{7}},
Pages = {{2185-2209}},
Month = {{JUL}},
Abstract = {{A law issued to combat political corruption and Mafia infiltration of
   city councils in Italy has resulted in episodes of large, unanticipated,
   temporary contractions in local public spending. Using these episodes as
   instruments, we estimate the output multiplier of spending cuts at
   provincial level-controlling for national monetary and fiscal policy,
   and holding the tax burden of local residents constant-to be 1.5.
   Assuming that lagged spending is exogenous to current output brings the
   estimate of the overall multiplier up to 1.9. These results suggest that
   local spending adjustment may be quite consequential for local activity.}},
Publisher = {{AMER ECONOMIC ASSOC}},
