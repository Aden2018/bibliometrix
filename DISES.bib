@article{ ISI:000075142400001,
Author = {Guiso, L and Jappelli, T},
Title = {{Background uncertainty and the demand for insurance against insurable
   risks}},
Journal = {{GENEVA PAPERS ON RISK AND INSURANCE THEORY}},
Year = {{1998}},
Volume = {{23}},
Number = {{1}},
Pages = {{7-27}},
Month = {{JUN}},
Abstract = {{Theory suggests that people facing higher uninsurable background risk
   buy more insurance against other risks that are insurable. This
   proposition is supported by Italian cross-sectional data. It is shown
   that the probability of purchasing casualty insurance increases with
   earnings uncertainty. This finding is consistent with consumer
   preferences being characterized by decreasing absolute prudence.}},
